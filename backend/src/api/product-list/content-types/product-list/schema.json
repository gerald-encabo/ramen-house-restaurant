{
  "kind": "collectionType",
  "collectionName": "product_lists",
  "info": {
    "singularName": "product-list",
    "pluralName": "product-lists",
    "displayName": "Product List"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "text"
    },
    "price": {
      "type": "float"
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "altImg": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Weekly Special",
        "Ramen",
        "Side",
        "Dessert",
        "Drink"
      ]
    }
  }
}
